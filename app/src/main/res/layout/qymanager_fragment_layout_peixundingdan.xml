                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="6dp"
                android:text="取消（0）"
                android:textSize="16sp"
                />
