                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/table_hight"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="查看"
                    android:textSize="@dimen/qyinfo_text_size"
                    android:textColor="@color/global_bg_blue"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/table_hight"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="企业人员"
                    android:textSize="@dimen/qyinfo_text_size"
                    android:textColor="@color/global_bg_blue"
                    />
            </LinearLayout>

    </TableRow>
        <TableRow>
