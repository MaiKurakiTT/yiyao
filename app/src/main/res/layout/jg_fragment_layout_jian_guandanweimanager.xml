<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/global_bg_gray"
    android:orientation="vertical">


    <!--查询条件和查询条件之下的输入框  start-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/sj_shenhe_select" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="40dp"
            android:text="@string/sj_shenhe_selectmsg"
            android:textColor="@color/global_black"
            android:textSize="@dimen/qyinfo_text_size" />

        <TextView
            android:id="@+id/select"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:background="@drawable/yellow_but_bg"
            android:clickable="true"
            android:paddingBottom="5dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="5dp"
            android:text="@string/sj_shenhe_select"
            android:textColor="@color/global_white" />
    </RelativeLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:paddingTop="10dp"
        android:paddingBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        android:background="@drawable/fillet_white_bg"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/qyinfo_text_size"
                android:text="用  户  名:"
                android:textColor="@color/global_black"
                android:layout_gravity="center_vertical"
                />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="@dimen/edit_height"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textSize="@dimen/qyinfo_text_size"
                android:layout_marginLeft="5dp"
                android:background="@drawable/table_border_bg"
                />

        </LinearLayout>


        <LinearLayout
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/qyinfo_text_size"
                android:text="注册时间:"
                android:textColor="@color/global_black"
                android:layout_gravity="center_vertical"
                />
            <EditText
                android:layout_width="110dp"
                android:layout_height="@dimen/edit_height"
                android:paddingTop="10dp"
                android:textSize="@dimen/qyinfo_text_size"
                android:paddingBottom="10dp"
                android:layout_marginLeft="5dp"
                android:background="@drawable/table_border_bg"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                />

            <EditText
                android:layout_width="100dp"
                android:layout_height="@dimen/edit_height"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textSize="@dimen/qyinfo_text_size"
                android:layout_marginRight="10dp"
                android:background="@drawable/table_border_bg"
                />
        </LinearLayout>


    </LinearLayout>


    <!--查询条件和查询条件之下的输入框  end-->


    <!--体验机构列表 start-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/sj_shenhe_menu" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="40dp"
            android:text="@string/sj_shenhe_tiyan"
            android:textColor="@color/global_black"
            android:textSize="@dimen/qyinfo_text_size" />

        <TextView
            android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:background="@drawable/yellow_but_bg"
            android:clickable="true"
            android:paddingBottom="5dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="5dp"
            android:text="新增"
            android:textColor="@color/global_white" />
    </RelativeLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/fillet_white_bg">

        <TableRow>
            <TextView
                android:layout_height="@dimen/table_hight"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:gravity="center"
                android:text="序号"
                android:background="@drawable/edit_border_bg"
                android:textSize="@dimen/qyinfo_text_size"
                android:textColor="@color/global_black"
                />
            <TextView
                android:layout_height="@dimen/table_hight"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:gravity="center"
                android:text="账号"
                android:background="@drawable/edit_border_bg"
                android:textSize="@dimen/qyinfo_text_size"
                android:textColor="@color/global_black"
                />
            <TextView
                android:layout_height="@dimen/table_hight"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:gravity="center"
                android:text="监管单位名称"
                android:background="@drawable/edit_border_bg"
                android:textSize="@dimen/qyinfo_text_size"
                android:textColor="@color/global_black"
                />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="@dimen/table_hight"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:gravity="center"
                android:text="1"
